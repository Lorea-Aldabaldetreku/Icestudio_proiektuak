        {
          "id": "15d48b94-6d86-4029-85fa-9b6873a34d5a",
          "type": "basic.code",
          "data": {
            "code": "// 24 biteko zenbaki bitarra hamartarra bihurtu\r\n// 24 biteko zenbakiak 8 digitu ematen ditu, 16777216 konbinazio ematen ditu\r\n// zenbaki digitala hamartarra biurtzeko blokea da honako hau. \r\n// Eta zenbaki hamartarra banatuko dugu 8 digituetan\r\n\r\n\r\nreg [3:0] i;\r\nreg [31:0] bcd;\r\n\r\nalways@(bitarra) begin\r\n \r\n bcd = 0; //bcd 0-ra hasi\r\n    \r\n    for (i = 0; i < 24; i = i+1) //24 aldiz errepikatu eragiketa 24 bitekin\r\n       \r\n        begin\r\n        \r\n        bcd = {bcd[30:0],bitarra[23-i]}; //bitak kateatu\r\n        \r\n        //cm, dm, hm, km,... 4 baino handiagoak badira, 3 gehitu \r\n        \r\n        if(i < 23 && bcd[3:0] > 4) \r\n         bcd[3:0] = bcd[3:0] + 3;\r\n        if(i < 23 && bcd[7:4] > 4)\r\n         bcd[7:4] = bcd[7:4] + 3;\r\n        if(i < 23 && bcd[11:8] > 4)\r\n         bcd[11:8] = bcd[11:8] + 3;  \r\n        if(i < 23 && bcd[15:12] > 4)\r\n         bcd[15:12] = bcd[15:12] + 3;\r\n        if(i < 23 && bcd[19:16] > 4)\r\n         bcd[19:16] = bcd[19:16] + 3;\r\n        if(i < 23 && bcd[23:20] > 4)\r\n         bcd[23:20] = bcd[23:20] + 3;\r\n        if(i < 23 && bcd[27:24] > 4)\r\n         bcd[27:24] = bcd[27:24] + 3;\r\n        if(i < 23 && bcd[31:28] > 4)\r\n         bcd[31:28] = bcd[31:28] + 3;\r\n\r\n\r\n        end\r\n end  \r\nassign d1 = bcd[3:0];\r\nassign d2 = bcd[7:4];\r\nassign d3 = bcd[11:8];\r\nassign d4 = bcd[15:12];\r\nassign d5 = bcd[19:16];\r\nassign d6 = bcd[23:20];\r\nassign d7 = bcd[27:24];\r\nassign d8 = bcd[31:28];\r\n",
